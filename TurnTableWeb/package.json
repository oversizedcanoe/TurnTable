{
  "name": "turn-table-web",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host=0.0.0.0",
    "start:prod": "ng serve --host=0.0.0.0 --configuration production",
    "start https": "ng serve --host=0.0.0.0 --ssl",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "predev": "if exist .angular\\cache rmdir /s /q .angular\\cache",
    "dev": "ng build && npm unlink ngx-cards && npm link ngx-cards && npm start",
    "dev https": "ng build && npm unlink ngx-cards && npm link ngx-cards && npm start https"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.0",
    "@angular/common": "^18.2.0",
    "@angular/compiler": "^18.2.0",
    "@angular/core": "^18.2.0",
    "@angular/forms": "^18.2.0",
    "@angular/platform-browser": "^18.2.0",
    "@angular/platform-browser-dynamic": "^18.2.0",
    "@angular/router": "^18.2.0",
    "@microsoft/signalr": "^8.0.7",
    "jest-editor-support": "*",
    "rxjs": "~7.8.0",
    "simple-keyboard": "^3.8.25",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.3",
    "@angular/cli": "^18.2.3",
    "@angular/compiler-cli": "^18.2.0",
    "@types/jasmine": "~5.1.0",
    "jasmine-core": "~5.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "tslib": "^2.8.1",
    "typescript": "~5.5.2"
  }
}
